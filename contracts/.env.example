# Arc Network Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# Required: Your deployer wallet private key (without 0x prefix)
# Generate a new wallet for development, never use your main wallet
PRIVATE_KEY=your_private_key_here

# Arc Testnet RPC URL
ARC_RPC_URL=https://arc-testnet.g.alchemy.com/v2/8twSrpdWnBUwCRVu9kzlr

# Optional: Alchemy API key if using custom endpoint
ALCHEMY_API_KEY=8twSrpdWnBUwCRVu9kzlr

# Optional: For contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# Deployed contract address (filled after deployment)
ESCROW_CONTRACT=
